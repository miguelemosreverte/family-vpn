# VPN Configuration
# Copy this file to .env and fill in your values
# Never commit .env to git!

# VPN Server
VPN_SERVER_HOST=your.server.ip.here
VPN_SERVER_PORT=8888

# SSH Configuration
VPN_SSH_USER=root

# SSH Private Key (base64 encoded)
# To get your key: cat ~/.ssh/your_key | base64
# To decode: echo "$VPN_SSH_KEY" | base64 -d > /tmp/key && chmod 600 /tmp/key
VPN_SSH_KEY=<paste_base64_encoded_ssh_key_here>

# Local Configuration (for sudo prompts)
SUDO_PASSWORD=your_password_here

# VPN Encryption Key (32 bytes for AES-256)
# Generate with: openssl rand -hex 32
VPN_ENCRYPTION_KEY=0123456789abcdef0123456789abcdef
